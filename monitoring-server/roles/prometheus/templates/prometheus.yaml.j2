global:
  scrape_interval: {{ SCRAPE_INTERAVL }}
  evaluation_interval: {{ EVALUATION_INTERNAL }}
  external_labels:
    monitor: {{ EXTERNAL_LABELS_MONITROR }}


scrape_configs:
  - job_name: prometheus
    static_configs:
      - targets: ['localhost:9090']

  - job_name: 'grafana'
    metrics_path: '/metrics'
    static_configs:
      - targets: ['grafana:3000']